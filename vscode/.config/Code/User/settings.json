  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "workbench.colorTheme": "Default Light+",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.accessibilitySupport": "off",
  "workbench.editor.empty.hint": "hidden",
  "editor.rulers": [120],
  "editor.cursorBlinking": "phase",
  "totalTypeScript.hideAllTips": false,
  "totalTypeScript.hideBasicTips": false,
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "stash-push:command"
  ],
  "editor.fastScrollSensitivity": 6,
  "editor.scrollbar.verticalScrollbarSize": 20,
  "editor.smoothScrolling": true,
  "editor.find.addExtraSpaceOnTop": false,
  "search.exclude": {
    "**/*.md": true,
    "**/*.spec.ts": true
  },
  "window.titleBarStyle": "custom",
  "workbench.tree.enableStickyScroll": true,
  "multiDiffEditor.experimental.enabled": true,
  "nxConsole.showNodeVersionOnStartup": false,
  "window.menuBarVisibility": "compact",
  "code-runner.temporaryFileName": "spvtemp",
  "better-comments.multilineComments": true,
  "better-comments.highlightPlainText": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.minimap.maxColumn": 60,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.autohide": true,
  "editor.lineHeight": 2.5,
  "javascript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.importModuleSpecifier": "relative",
  "prisma.showPrismaDataPlatformNotification": false,
  "gopls": {
    "ui.semanticTokens": true
  },
  "go.toolsManagement.autoUpdate": true,
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },
  "redhat.telemetry.enabled": false,
  "editor.formatOnSave": true,
  "files.participants.timeout": 0,
  "editor.minimap.size": "fill",
  "database-client.autoSync": true,
  "gitlens.views.scm.grouped.views": {
    "commits": true,
    "branches": true,
    "remotes": true,
    "stashes": false,
    "tags": true,
    "worktrees": true,
    "contributors": true,
    "repositories": true,
    "searchAndCompare": false,
    "launchpad": false
  },
  "codeium.disableSupercomplete": true,
  "editor.copyWithSyntaxHighlighting": false,
  "terminal.integrated.fontLigatures": true,
  "typescript.experimental.expandableHover": true,
  "terminal.integrated.suggest.enableExtensionCompletions": true,
  "workbench.editor.enablePreview": false,
  "codeium.enableCodeLens": false,
  "prisma.enableCodeLens": true,
  "editor.occurrencesHighlightDelay": 1000,
  "terminal.integrated.suggest.quickSuggestions": {
    "commands": "on",
    "arguments": "on",
    "unknown": "off"
  }
